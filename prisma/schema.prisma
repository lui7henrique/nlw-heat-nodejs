// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String @id @default(uuid())
  name       String
  login      String
  avatar_url String
  url        String
  location   String
  bio        String
  github_id  Int

  @@map("users")
}
